    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100&display=swap" rel="stylesheet">
    {% block stylesheet %}{% endblock %}
    <link rel="icon" href="/assets/images/favicon.png">
</head>

<body>

    <header>
        {% block nav %}{% endblock %}
    </header>

    <main>
        {% block content %}{% endblock %}
